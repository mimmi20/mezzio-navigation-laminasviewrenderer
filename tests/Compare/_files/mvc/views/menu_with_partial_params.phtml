<?php
$isContainer = $this->vars('container') instanceof \Mimmi20\Mezzio\Navigation\ContainerInterface
    ? 'yes'
    : 'no';
echo $this->variable . PHP_EOL;
$pages = [];
foreach ($this->vars('container') as $page) {
    $pages[] = $page->getLabel();
}
$pages = implode(', ', $pages);
?>
Is a container: <?php echo $isContainer . PHP_EOL; ?>
Pages: <?php echo $pages; ?>
