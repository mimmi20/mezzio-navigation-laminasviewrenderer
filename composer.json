{
  "name": "mimmi20/mezzio-navigation-laminasviewrenderer",
  "type": "library",
  "description": "Provides viewhelper for Mezzio Navigation",
  "homepage": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "laminas/laminas-log": "^2.12.0",
    "laminas/laminas-stdlib": "~3.2.1 || ~3.3.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.2.0",
    "mimmi20/mezzio-navigation": "dev-updates",
    "psr/container": "^1.0.0"
  },
  "require-dev": {
    "infection/infection": "~0.13.6 || ~0.18",
    "laminas/laminas-i18n": "^2.10.3",
    "laminas/laminas-uri": "^2.7.1",
    "laminas/laminas-validator": "^2.13.4",
    "mezzio/mezzio-router": "^3.1.3",
    "mimmi20/coding-standard": "~1.0.5",
    "mimmi20/mezzio-generic-authorization": "dev-master",
    "pepakriz/phpstan-exception-rules": "~0.11.2",
    "phpstan/extension-installer": "~1.0.5",
    "phpstan/phpstan": "~0.12.56",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpunit/phpunit": "~7.5.20 || ~8.5.8 || ~9.4.2"
  },
  "suggest": {
    "ext-dom": "required to use the Sitemap ViewHelper",
    "laminas/laminas-i18n": "^2.10.3, required to use the Menu ViewHelper",
    "laminas/laminas-uri": "^2.7.1, required to use the Sitemap ViewHelper",
    "laminas/laminas-validator": "^2.13.4, required to use the Sitemap ViewHelper"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\Navigation\\LaminasView\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\Navigation\\LaminasView\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\Navigation\\LaminasView\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer/issues",
    "source": "https://github.com/mimmi20/mezzio-navigation-laminasviewrenderer"
  }
}
